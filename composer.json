{
    "name": "mvieira/json-collection",
    "description": "PHP implementation of the Collection.next+JSON Media Type",
    "version": "0.0.1",
    "type": "library",
    "homepage": "https://github.com/mickaelvieira/JsonCollection",
    "minimum-stability": "stable",
    "keywords": [
        "Hypermedia", "RESTful", "HTTP", "JSON", "REST", "HATEOAS"
    ],
    "authors": [
        {
            "name": "Mickael Vieira",
            "email": "contact@mickael-vieira.com",
            "homepage": "http://mickael-vieira.com"
        }
    ],
    "require": {
        "php": "~5.4"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "phpspec/nyan-formatters": "1.*",
        "henrikbjorn/phpspec-code-coverage": "~0.2",
        "squizlabs/php_codesniffer": "1.*",
        "phpmd/phpmd" : "2.0.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "JsonCollection\\": "src/JsonCollection/"
        }
    }
}